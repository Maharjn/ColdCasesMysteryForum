-vh\">\r\n          <Header />\r\n          <div className=\"container\">\r\n            <Routes>\r\n              <Route \r\n                path=\"/\"\r\n                element={<Home />}\r\n              />\r\n              <Route \r\n                path=\"/login\" \r\n                element={<Login />}\r\n              />\r\n              <Route \r\n                path=\"/signup\" \r\n                element={<Signup />}\r\n              />\r\n              <Route \r\n                path=\"/me\" \r\n                element={<Profile />}\r\n              />\r\n              <Route \r\n                path=\"/profiles/:username\" \r\n                element={<Profile />}\r\n              />\r\n              <Route \r\n                path=\"/thoughts/:thoughtId\" \r\n                element={<SingleThought />}\r\n              />\r\n            </Routes>\r\n          </div>\r\n         \r\n        </div>\r\n      </Router>\r\n    </ApolloProvider>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\reportWebVitals.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\pages\\SingleThought.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\pages\\Home.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\pages\\Profile.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\pages\\Login.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\pages\\Signup.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\components\\Header\\index.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\components\\Footer\\index.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\\ColdCasesMysteryForum\\client\\src\\utils\\mutations.js",[],"C:\\Users\\shivr\\OneDrive\\Desktop\\maha\